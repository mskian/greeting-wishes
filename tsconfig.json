{
    "compilerOptions": {
      "target": "ES6",
      "lib": ["ES6", "DOM"],
      "module": "CommonJS",
      "rootDir": "src",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "include": [
    "src/**/*",
    "src/canva/**/*"
    ],
    "exclude": ["node_modules", "dist"]
}